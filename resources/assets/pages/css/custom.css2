#shareModal .shareHead{padding:12px 10px; border-radius:4px; background-color:#fff;}
#shareModal .shareBody{padding:10px; border-radius:4px; background-color:#fff; margin:10px 0 0 0;}
#shareModal .shareHead .catagoryTagRow{margin-bottom:0; padding-bottom:0;}
#shareModal .shareselect{display:block; margin:10px 0 0 0; padding:12px 10px; background-color:#fff; border-radius:4px;} 
#shareModal .modalShareFooter{display:block; margin:10px 0 0 0; }
#shareModal .shareHead .shareInput{margin:0;}
#shareModal .cardMessageType .textBx{border:none;}
#shareModal .shareselect .form-group-default{padding:0; border:none !important; margin:0;}
#shareModal .modalShareFooter .btn{font-weight:600 !important; font-size:15px !important;}
.nearbyPage{display:block;}
.showNearby{display:inline-block;}
.nearbyPage .showNearby{display:inline-block;}
.cardDetailsPG_modal.modal{position:static; display:block !important; overflow:visible;}
.cardDetailsPG_modal.modal .modal-body{padding:0;}
.userIsTyping{display:block;color:#222;font-size:12px;line-height:14px;font-weight:500;margin: 0;height: 15px;}
.modalComments  form{padding-top:6px;}
.container-fluid.postDetailsPG{background-color:#fff;}
.scrollTabOuter.detailsTopbar{/* translateY(60px) */}
.detailsTopbar {display:none; height:41px;}
.detailsTopbar .mobileModalHeader{-webkit-box-shadow:none; box-shadow:none; height:41px;  position:fixed; -webkit-transform:translateY(60px); transform:translateY(60px); -webkit-transition:600ms; transition:600ms; z-index:9;}
.suggestedUsers{display:block; padding:12px 25px 2px 25px; position:relative; margin:0 -25px; position:relative; /* background-color:#f7f7f7; */}
.suggestedUsers .userList{display:block; margin:0; font-size:0; text-align:center; outline:none;}
.suggestedUsers .userList .list{display:inline-block;vertical-align:top;width: 160px;padding:0 0 0 10px;margin-right:-1px;}
.suggestedUsers .userList .list .bX{display:block;border:1px solid #ddd;border-radius:4px;padding: 10px 10px 8px 10px; background-color:#fff;}
.suggestedUsers .userStatusInfoTtl{font-size:14px; line-height:16px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.suggestedUsers .userAbout{font-size:13px; margin:4px 0 0 0 !important;}
.suggestedUsers .userAbout.postTech{font-size: 11px;line-height:13px;margin: 1px 0 0 0 !important;display: block; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.suggestedUsers .userStatusRow{border:none; padding:0; margin:0;}
