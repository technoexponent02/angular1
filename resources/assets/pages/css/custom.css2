@charset "utf-8";
/* CSS Document */
body,
*{font-family: 'Open Sans', sans-serif;}
h1, h2, h3, h4, h5, h6,
h1 strong, h2 strong, h3 strong, h4 strong, h5 strong, h6 strong,
.userStatusInfoTtl a,
.catagoryTtl p,
.profileTtl a,
.profileNav ul li a strong,
.profileNav .item a strong, 
.widget-advanced .widget-content-image.profileName,
.widget-advanced .widget-content-image.profileName a,
.uploadFile span,
.cardPreviewTxt{font-family: 'Montserrat', sans-serif;}
.windows h2,
.windows h2 strong,
.register-container h2,
.register-container h2 strong{font-family: 'Open Sans', sans-serif;}
.header .header-inner.qickVwNave{display:block; float:right; width:auto;}
.header .header-inner.qickVwNave .btn-link{margin:22px 0 0 12px;}
.topHeadBtns {margin:12px 0 0 0; display:none; float:left;}
.topHeadBtns .btn{margin:0 8px 0 0;}
.userNav{display:block; float:left; margin:8px 0 0 16px;}
.header .notification-list{vertical-align:middle;}
.notificationList{width:400px;}
.notificationList .more-details-inner h5{margin-top:0;}
.notifiHead{display:block; position:relative; padding:0 104px 0 0;}
.notifiHead .pull-right{display:block; position:absolute; top:5px; right:0; width:98px; text-align:right; line-height:18px;}
.notifiHead .pull-left i,
.notifiHead .pull-left span{display:block; float:left; line-height:26px;}
.notifiHead .pull-right .circular{margin-top:0;}
.notifiHead .pull-right .time{vertical-align:top; line-height:18px;}
.toggle-more-details > div{height:14px; line-height:14px;}
.innerTab{padding:0; text-align:center;}
.innerTab li{display:inline-block; vertical-align:top; float:none;}
#hero-image .cover-photo{text-align:center; padding:72px 20px 72px; position:relative; -webkit-transform:inherit !important; transform:inherit !important;}
#hero-image .cover-photo h2{margin-bottom:15px;}
#hero-image .cover-photo #hero-close-button{position:absolute; top:12px; right:12px;}
#hero-image .cover-photo .hero-image-text{width:100%; font-size:40px; margin-left:0; font-weight:600;font-family: "Montserrat" !important; margin:0;}
#hero-image .cover-photo .btn{display:inline-block; margin:6px 16px;	border:1px solid #ccc; border-radius:45px; font-size:18px !important; font-weight:500 !important; height:40px; line-height:38px !important; padding:0 20px; /* font-family: "Montserrat" !important; */}
.gallerySection .gallery-item{overflow:hidden;}
.gallerySection .gallery-item > img{position:relative; opacity:0;}
.gallerySection .gallery-item > img.fWidth{width:100%; height:auto;}
.gallerySection .gallery-item > img.fHeight{height:100%; width:auto;}
.gallerySection .gallery-item .gradient-grey > .m-t-10{position:relative; padding-right:48px; padding-left:35px;}
.gallerySection .gallery-item .m-t-10 .pull-right.m-t-10{position:absolute; top:0; right:0;/*  margin:0; */}
.gallerySection .gallery-item .gradient-grey > .m-t-10 .inline.m-l-10{max-width:100%;}
.gallerySection .gallery-item .gradient-grey > .m-t-10 .inline.m-l-10 .no-margin{overflow:hidden; text-overflow:ellipsis; white-space:nowrap; max-width:100%;}
.gallerySection .gallery-item .m-t-10 .thumbnail-wrapper{position:absolute; top:0; left:0;/*  margin:0; */}
.categorySlider{position:relative; padding:0; height:94px; overflow:hidden;}
.owl-dots{position:absolute; top:100%; margin:20px 0 0 0;}
.owl-dot{width:12px; height:12px; background:#000;}
.categorySlider .item{padding:0;}
.categorySlider .owl-pagination{display:none;}
.categorySlider .owl-prev,
.itemPrev,
.pickCatrgorySliderSec .slick-prev{font-size:0px; width:62px; height:100%; position:absolute; top:0;	left:4px;
	background-image: -webkit-linear-gradient(left,rgba(0,0,0,.7) 0,rgba(0,0,0,.0001) 100%);
	background-image: -o-linear-gradient(left,rgba(0,0,0,.7) 0,rgba(0,0,0,.0001) 100%);
	background-image: -webkit-gradient(linear,left top,right top,from(rgba(0,0,0,.7)),to(rgba(0,0,0,.0001)));
	background-image: linear-gradient(to right,rgba(0,0,0,.7) 0,rgba(0,0,0,.0001) 100%);
	background-repeat: repeat-x;
	-moz-border-radius:5px 0 0 5px;
	border-radius:5px 0 0 5px;
	z-index: 999;
}
.categorySlider .owl-next,
.itemNext,
.pickCatrgorySliderSec .slick-next{font-size:0px; Width:62px; height:100%; position:absolute; top:0;	right:4px;
	background-image: -webkit-linear-gradient(right,rgba(0,0,0,.7) 0,rgba(0,0,0,.0001) 100%);
	background-image: -o-linear-gradient(right,rgba(0,0,0,.7) 0,rgba(0,0,0,.0001) 100%);
	background-image: -webkit-gradient(linear,right top,left top,from(rgba(0,0,0,.7)),to(rgba(0,0,0,.0001)));
	background-image: linear-gradient(to left,rgba(0,0,0,.7) 0,rgba(0,0,0,.0001) 100%);
	background-repeat: repeat-x;
	-moz-border-radius:0 5px 5px 0;
	border-radius:0 5px 5px 0;
	z-index: 999;
}
.itemPrev,
.itemNext{cursor:pointer; display:none !important;}
.pickCatrgorySliderSec .slick-next,
.pickCatrgorySliderSec .slick-prev{font-size:0px; -webkit-transform:translate(0,0); transform:translate(0,0);}
.pickCatrgorySliderSec .slick-next:before,
.pickCatrgorySliderSec .slick-prev:before{display:none;}
.pickCatrgorySliderSec{display:block; overflow:hidden; height:94px; position:relative; /* border-radius:5px; */}
.pickCatrgorySliderSec .pickCatrgorySlider{opacity:0;}
.catgLoad{display:block; position:absolute; top:0; left:0; width:100%;height:81px; background:url(https://swolk.com/assets/pages/img/progress/progress-circle-success.gif) no-repeat center center; background-size:24px auto;}
.pickCatrgorySliderSec.loaded .pickCatrgorySlider{opacity:1;}
.pickCatrgorySliderSec.loaded .catgLoad{display:none;}
.pickCatrgorySlider{display:block; margin:0 -4px; position:relative;}
.pickCatrgorySliderSec .item{padding:0 4px;}
.pickCatrgorySliderSec .slick-dots{display:none;}
.categorySlider:hover .owl-prev,
.categorySlider:hover .owl-next{display:block;}
.categorySlider .owl-prev i,
.categorySlider .owl-next i{display:none;}
.categorySlider .owl-prev:before,
.categorySlider .owl-next:before,
.itemNext:before,
.itemPrev:before,
.pickCatrgorySliderSec .slick-next:after,
.pickCatrgorySliderSec .slick-prev:after{content:"\f104";font-family:FontAwesome;display:block;position:absolute;top:50%;left:50%;color: #E6E6E6;font-size: 48px;line-height:30px;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%);}
.categorySlider .owl-next:before,
.itemNext:before,
.pickCatrgorySliderSec .slick-next:after{content:"\f105";}
.catgBx{display:block; position:relative; height:94px; overflow:hidden; border-radius:5px; background-position:center center !important; background-size:cover !important;}
.catgBxImage{display:block; position:absolute; top:0; left:0; width:100%; height:100%; background-repeat:no-repeat !important;  background-position:center center !important; background-size:cover !important; border-radius:5px; overflow:hidden;}
.catgBx img{display:block; width:100%; height:auto;}
.catgttl{display:block;position: absolute;top: 0;left: 0;width: 100%;height: 100%;color: #fff;font-size: 13px;font-weight: 700;line-height: 14px;padding: 10px;text-align: center;border-radius:5px;background-color: rgba(0,0,0,0.4);}
.flickity-slider .item a:hover .catgttl,
.flickity-slider .item a:focus .catgttl{background-color: rgba(0,0,0,0.4);}
.item a:hover .catgttl,
.flickity-slider .item.sl a .catgttl{background-color: rgba(0,0,0,0.0);}
.item a.catselected .catgttl{background-color: rgba(0,0,0,0.0) !important;}
.catgttl > span{display:block;position:absolute;top:50%;left:0;width:100%;text-align:center;-webkit-transform:translate(0,-50%);transform:translate(0,-50%);padding:10px;font-size:16px; line-height:19px;/* text-transform: uppercase; */font-weight: 600; word-wrap:break-word; text-transform:capitalize;}
.gallerySection .col-sm-4{width:16.666666%; float:left;}
.scrollTab{display:block; height:45px; overflow:hidden; text-align:center; font-size:0px; border-bottom:1px solid rgba(0, 0, 0, 0.1); background-color:#ffffff; position:absolute; width:100%; top:0; left:0; z-index:21; /* -webkit-box-shadow:0 2px 4px 0 rgba(0, 0, 0, 0.09); box-shadow:0 2px 4px 0 rgba(0, 0, 0, 0.09); */ z-index:799;}
.scrollTab,
.scrollTab *{outline:none;}
.scrollTabNEW{display:block; position:fixed; top:59px;  z-index:2; visibility:hidden;}
.scrollTab .nav-tabs > li > a{padding:12px 20px 6px 20px !important; line-height:20px;}
.scrollTab .nav-tabs > li > a img{display:inline-block; vertical-align:middle; width:19px; margin:-3px 6px 0 0; opacity:0.7;}
.scrollTab .innerTab{display:block !important; vertical-align:top; font-size:0px; text-align:center; overflow:hidden;}
.scrollTab .innerTab li{display:inline-block !important; float:none !important; vertical-align:top;}
.scrollTab .innerTab:before,
.scrollTab .innerTab:after{display:none;}
.pace-done .explorTabSlider{opacity:1;}
.explorTabSlider .itm a,
.searchFechrBar .itm a{display:block; color:#909090; font-size:13px; line-height:20px; font-weight:600; padding:12px 24px;position:relative; text-transform:uppercase;}
.explorTabSlider .itm a .txt,
.searchFechrBar .itm a .txt{display:block; position:relative; padding:0 0 0 25px; white-space:nowrap;}
.searchFechrBar .itm a .txt{padding-left: 0;}
.explorTabSlider .itm a img,
.searchFechrBar .itm a img{display:block; position:absolute; top:50%; left:0; width:19px; margin:0; opacity:1; -webkit-transform:translate(0,-50%); transform:translate(0,-50%); -webkit-filter: grayscale(100%); filter: grayscale(100%);}
.explorTabSlider .itm a:after,
.searchFechrBar .itm a:after{content:""; display:none; position:absolute; bottom:0; left:0; width:100%; height:3px; background-color:#000;}
.explorTabSlider .itm.active a:after,
.explorTabSlider .itm.sel a:after,
.searchFechrBar .itm.active a:after,
.searchFechrBar .itm.sel a:after{display:block;}
.explorTabSlider .itm.active a,
.explorTabSlider .itm.sel a,
.searchFechrBar .itm.active a,
.searchFechrBar .itm.sel a{color:#000;}
.explorTabSlider .itm.active a img,
.explorTabSlider .itm.sel a img,
.searchFechrBar .itm.active a img,
.searchFechrBar .itm.sel a img{color:#f55753; -webkit-filter: grayscale(0%); filter: grayscale(0%); display:none;}
.explorTabSlider .itm a img, 
.searchFechrBar .itm a img{display:none;}
.explorTabSlider .itm a .txt, 
.searchFechrBar .itm a .txt{padding-left:0 !important;}

.explorTabSlider .itm a .nm,
.searchFechrBar .itm a .nm{display:block; position:relative;}
.explorTabSlider .itm a .thin,
.searchFechrBar .itm a .thin{display:block; position:absolute; top:0; left:0; width:100%; font-weight:500;}
.explorTabSlider .itm a .strong,
.searchFechrBar .itm a .strong{display:block; opacity:0; font-weight:700;}
.explorTabSlider .itm.active a .thin, 
.explorTabSlider .itm.sel a .thin, 
.searchFechrBar .itm.active a .thin, 
.searchFechrBar .itm.sel a .thin{opacity:0;}
.explorTabSlider .itm.active a .strong, 
.explorTabSlider .itm.sel a .strong, 
.searchFechrBar .itm.active a .strong, 
.searchFechrBar .itm.sel a .strong{opacity:1;}

